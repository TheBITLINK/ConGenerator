<Window x:Class="ConGenerator.ConfirmDelete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="{Binding XPath=/tbl/data/mesages/rm}" Height="233.597" Width="450.593" ResizeMode="NoResize">
    <Window.DataContext>
        <Binding Source="{StaticResource thislang}"/>
    </Window.DataContext>
    <Grid>
        <Button Content="{Binding XPath=/tbl/data/mesages/yes}" HorizontalAlignment="Left" Height="25" Margin="362,170,0,0" VerticalAlignment="Top" Width="73" IsDefault="True" Cursor="Hand" Click="Button_Click"/>
        <Button x:Name="cancel" Content="{Binding XPath=/tbl/data/mesages/no}" HorizontalAlignment="Left" Height="25" Margin="268,170,0,0" VerticalAlignment="Top" Width="89" Cursor="Hand" IsCancel="True"/>
        <Label Content="{Binding XPath=/tbl/data/mesages/rm}" HorizontalAlignment="Left" Height="32" Margin="10,10,0,0" VerticalAlignment="Top" Width="384" FontSize="16" Foreground="#FF81B200"/>
        <Label x:Name="maintext" Content="{Binding XPath=/tbl/data/mesages/confrimrm}" HorizontalAlignment="Left" Height="92" Margin="10,47,0,0" VerticalAlignment="Top" Width="425" Foreground="#FF787878"/>
    </Grid>
</Window>
